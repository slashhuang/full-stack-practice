  "bugs": {
    "url": "https://github.com/slashhuang/full-stack-practice/issues"
  },
  "homepage": "https://github.com/slashhuang/full-stack-practice#readme",
  "devDependencies": {
    "debug": "^2.6.3",
    "node-inspector": "^1.0.0",
    "nodemon": "^1.11.0"
  }
}
