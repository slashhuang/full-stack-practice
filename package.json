{
  "name": "full-stack",
  "version": "1.0.0",
  "description": "a full-stack-practice-for-node",
  "main": "index.js",
  "scripts": {
    "listener": "nodemon --inspect ./http/listener",
    "incoming": "nodemon --inspect ./http/incoming",
    "response": "nodemon --inspect ./http/response",


    "debug": "DEBUG=http,worker nodemon ./debug/debug.js",
    "inspector": "nodemon ./node_modules/.bin/node-debug ./debug/node-inspector.js",
    "repl":"nodemon debug ./debug/repl.js",

    "stream":"nodemon ./stream/stream.js",
    "buffer":"nodemon ./buffer/buffer.js",
    "v8":"nodemon ./v8/v8.js",

    "test":"nodemon ./test"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slashhuang/full-stack-practice.git"
  },
  "keywords": [
    "node",
    "posix"
  ],
